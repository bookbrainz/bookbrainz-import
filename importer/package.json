{
	"name": "bookbrainz-import",
	"version": "0.1.0",
	"description": "Import scripts for various data sources",
	"main": "index.js",
	"scripts": {
		"build": "rimraf lib/* && babel src --out-dir lib",
		"nodemon": "nodemon --watch './src/' --exec 'npm run build'",
		"lint": "eslint ./src",
		"consumer": "node ./lib/consumer/index.js",
		"importOLWorks": "node ./lib/openLibrary/producer/index.js -d works",
		"importOLAuthors": "node ./lib/openLibrary/producer/index.js -d authors",
		"importOLEditions": "node ./lib/openLibrary/producer/index.js -d editions",
		"precommit": "lint-staged"
	},
	"lint-staged": {
		"*.js": [
			"eslint"
		]
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/bookbrainz/bookbrainz-import.git"
	},
	"keywords": [
		"bookbrainz",
		"book",
		"data",
		"import"
	],
	"author": "Shivam Tripathi",
	"license": "GPL-2.0+",
	"dependencies": {
		"amqplib": "^0.5.3",
		"async": "^2.6.0",
		"bluebird": "^3.5.1",
		"bookbrainz-data": "^2.2.1",
		"franc-min": "^4.1.0",
		"immutable": "^4.0.0-rc.12",
		"lodash": "^4.17.11",
		"validator": "^11.0.0",
		"winston": "^3.2.1",
		"yargs": "^13.2.4"
	},
	"devDependencies": {
		"babel-cli": "^6.26.0",
		"babel-eslint": "^10.0.1",
		"babel-plugin-array-includes": "^2.0.3",
		"babel-plugin-lodash": "^3.3.4",
		"babel-plugin-transform-class-properties": "^6.24.1",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
		"babel-plugin-transform-object-rest-spread": "^6.26.0",
		"babel-preset-env": "^1.7.0",
		"babel-preset-flow": "^6.23.0",
		"babel-preset-stage-2": "^6.24.1",
		"eslint": "^5.16.0",
		"eslint-plugin-babel": "^5.3.0",
		"eslint-plugin-flowtype": "^3.10.3",
		"eslint-plugin-import": "^2.17.3",
		"flow": "^0.2.3",
		"flow-bin": "^0.101.0",
		"husky": "^2.4.1",
		"knex": "^0.14.6",
		"lint-staged": "^8.2.1",
		"nodemon": "^1.19.1",
		"rimraf": "^2.6.3"
	}
}
